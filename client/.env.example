# API Configuration
VITE_API_URL=http://localhost:3000

# Timeout Configuration (in milliseconds)
# General API requests timeout (default: 60000 = 1 minute)
VITE_API_TIMEOUT=60000

# Upload timeout for large files (default: 600000 = 10 minutes)
VITE_UPLOAD_TIMEOUT=600000

# File Processing Configuration
# Large file warning threshold in MB (default: 50)
VITE_LARGE_FILE_THRESHOLD_MB=50

# Chunk size for file uploads in bytes (default: 1048576 = 1MB)
VITE_DEFAULT_CHUNK_SIZE=1048576